# Ignore macOS system files
.DS_Store

# Ignore node_modules if using npm or yarn
node_modules/

# Ignore dependency lock files (if using npm/yarn)
package-lock.json
yarn.lock

# Ignore generated build files (if using a build process)
dist/
build/

# Ignore logs and temporary files
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Ignore IDE/editor-specific files
.vscode/
.idea/
*.sublime-project
*.sublime-workspace

# Ignore cache and temporary files
.cache/
.tmp/

# Ignore Sass/SCSS cache files
.sass-cache/

# Ignore dependency manager files (if using Ruby/Bundler)
vendor/

# Ignore GitHub Pages Jekyll-generated site files
_site/
.jekyll-cache/

# Ignore compiled CSS if using pre-processors
*.map
